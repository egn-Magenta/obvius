function update_hidden_widget(widget, hidden_widget) {
    var new_value="";
    var i;

    for(i=0; i<widget.options.length; i++) {
        new_value=new_value + (i ? "\n" : "") + widget.options[i].value;
    }

    hidden_widget.value=new_value;
}

function insert_all(widget, fields, hidden_widget) {
    var i;

    widget.options.length=0;
    for(i=0; i<fields.options.length; i++) {
        add_field(widget, fields.options[i].value, fields.options[i].value, hidden_widget);
    }
    update_hidden_widget(widget, hidden_widget)
}

function insert_fields(widget, fields, hidden_widget) {
    var i;
    widget.options.length=0;
    for(i=0; i<fields.length; i++) {
        if (fields[i]) {
            add_field(widget, fields[i].field, fields[i].name, hidden_widget);
	}
    }
    update_hidden_widget(widget, hidden_widget)
}

function add_field(widget, field, text, hidden_widget) {
    if (text==null || text=='') {
        text=field;
    }
    var fieldvalue=field + ':' + text;
    var newoption = new Option(text, fieldvalue);
    widget.options[widget.options.length]=newoption;

    update_hidden_widget(widget, hidden_widget);
}

function remove_field(widget, num, hidden_widget) {
    widget.options[num]=null;
    if (widget.options.length==num ) {
        num--;
    }
    if (widget.options.length>num) {
        widget.selectedIndex=num;
    }
    update_hidden_widget(widget, hidden_widget);
}

function move_field_up(widget, num, hidden_widget) {
    if (num>=1) {
        var curoption=widget.options[num];
        var prevoption=widget.options[num-1];
        widget.options[num-1]=new Option(curoption.text, curoption.value);
        widget.options[num]=new Option(prevoption.text, prevoption.value);
        widget.selectedIndex=num-1;
	update_hidden_widget(widget, hidden_widget);
    }
}

function move_field_down(widget, num, hidden_widget) {
    if (num>=0 && num<(widget.options.length-1)) {
        var curoption=widget.options[num];
        var nextoption=widget.options[num+1];
        widget.options[num+1]=new Option(curoption.text, curoption.value); 
        widget.options[num]=new Option(nextoption.text, nextoption.value);
        widget.selectedIndex=num+1;
	update_hidden_widget(widget, hidden_widget);
    }
}

function init_fields_<% $name %>() {
% foreach (@values) {
%     chomp;
%     my($field, $text)=split /:/;
    add_field(window.document.pageform.<% $name %>_internaljsfieldlist, "<% $field %>", "<% $text %>", window.document.pageform.<% $name %>);
% }
}

init_fields_<% $name %>();
</script>

% } else {
<& .fieldchooser, name=>$name, fields=>\@fields &>
% }
%
<%init>
my $value=$session->{fields_out}->param(uc($name));
   $value=$session->{fields_in}->param(uc($name)) unless defined($value);

my @values;
use Data::Dumper;
#print STDERR " EDIT value $name: " . Dumper($value);
#print STDERR " ref value  $name:  " . ref($value) . "\n";
# Abusing the repeatable flag:
if ($value and @$value == 1) {
    $value->[0] =~ s/\r//g;
    @values=split /\n/, $value->[0];
}
#print STDERR " EDITNOW value $name: " . Dumper(\@values);

# Determine doctype of subdocuments:
my $doc=$session->{document};
my $vdoc=$session->{version};
my $doctypeid;
if (my  $selectdoctypename=($session->{fields_out}->param('SELECTDOCTYPE') ||
			    $session->{fields_in}->param('SELECTDOCTYPE') ||
			    $vdoc->field('selectdoctype'))) {
    if (my $selectdoctype=$obvius->get_doctype_by_name($selectdoctypename)) {
	$doctypeid=$selectdoctype->Id;
    }
}
unless (defined $doctypeid) {
    my $subdocs=$obvius->get_docs_by_parent($doc->Id);
    if ($subdocs) {
	foreach (@$subdocs) {
	    my $subvdoc=$obvius->get_public_version($_) || $obvius->get_latest_version($_);
	    if ($doctypeid) {
		warn "Subdocuments of different types" if ($doctypeid != $subvdoc->Type);
	    }
	    else {
		$doctypeid=$subvdoc->Type;
	    }
	}
    }
}

my $subvdoctype;
my $doctypename;
my @fields;
if (defined $doctypeid) {
    $subvdoctype=$obvius->get_doctype($doctypeid);
    $doctypename=$subvdoctype->Name;
    print STDERR " doctypename: $doctypename\n";
    foreach ($subvdoctype->{FIELDS}->param) {
	push @fields, lc($_) unless $_ =~ /_comment$/i;
    }
}

# Find editpage:
my $editpage=(defined $subvdoctype ? $obvius->get_editpages($subvdoctype)->{ (scalar(keys %{$obvius->get_editpages($subvdoctype)})-1 ) } : undef);

my $site=$r->pnotes('site');
my $fieldlist=(defined $editpage ? $site->parse_editpage_fieldlist($editpage->Fieldlist, $subvdoctype, $obvius) : undef);
</%init>
</%method>
